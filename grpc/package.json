{
  "name": "selfservicedesk-grpc",
  "version": "1.0.0",
  "author": "Lee Boonstra",
  "license": "Apache-2.0",
  "description": "Code sample for STT/Dialogflow app back-end.",
  "engines": {
    "node": ">=10.9.0 <13.0.0",
    "yarn": ">=1.17.3 <=1.19.1"
  },
  "scripts": {
    "start": "node dist/grpc/index.js",
    "watch": "nodemon --watch \"**/*.ts\" --exec \"ts-node\" index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc -p .",
    "compile-proto": "mkdir -p pbjs-genfiles && pbjs -p ./node_modules/google-proto-files google/cloud/dialogflow/v2beta1/session.proto -o ./pbjs-genfiles/proto.js -t static-module"
  },
  "private": true,
  "dependencies": {
    "dotenv": "^8.2.0",
    "google-auth-library": "^5.5.1",
    "google-proto-files": "^1.1.1",
    "node-fetch": "^2.6.0",
    "protobufjs": "^6.8.8"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "3.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^12.12.14",
    "@types/uuid": "^3.4.6",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.2",
    "tsify": "^4.0.1",
    "tslint": "~5.20.1",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  }
}
